<odoo>
    <data>
        <!-- Form view for ManagedModel -->
        <record id="view_managed_model_form" model="ir.ui.view">
            <field name="name">managed.model.form</field>
            <field name="model">managed.model</field>
            <field name="arch" type="xml">
                <form string="Model">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="description"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_managed_model_tree" model="ir.ui.view">
            <field name="name">managed.model.tree</field>
            <field name="model">managed.model</field>
            <field name="arch" type="xml">
                <tree string="Managed Models">
                    <field name="name"/>
                    <field name="description"/>
                </tree>
            </field>
        </record>

        <!-- Action to open ManagedModel form view -->
        <record id="action_managed_model_form" model="ir.actions.act_window">
            <field name="name">Managed Models</field>
            <field name="res_model">managed.model</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Click to create a new managed model.
                </p>
            </field>
        </record>

        <!-- Menu item to open ManagedModel form view -->
        <menuitem id="menu_managed_model_root" name="Managed Service" sequence="30" web_icon="managed_service,static/description/icon.png"/>

        <menuitem id="menu_managed_model" name="Managed Service" parent="menu_managed_model_root" sequence="10"/>
        
        <menuitem id="menu_managed_model_form" name="Models" parent="menu_managed_model" action="action_managed_model_form" sequence="2"/>
    
    </data>
</odoo>
